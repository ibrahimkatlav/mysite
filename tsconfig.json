{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es6",
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "jsx": "preserve",
    "isolatedModules": true,
    "incremental": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      
      "astro/client"
    ],
    "paths": {
      "@/components/*": [
        "./src/layouts/components/*"
      ],
      "@/shortcodes/*": [
        "./src/layouts/shortcodes/*"
      ],
      "@/helpers/*": [
        "./src/layouts/helpers/*"
      ],
      "@/partials/*": [
        "./src/layouts/partials/*"
      ],
      "@public/*": [
        "./public/images/blog/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.astro"
  ],
  "exclude": [
    "node_modules"
  ]
}